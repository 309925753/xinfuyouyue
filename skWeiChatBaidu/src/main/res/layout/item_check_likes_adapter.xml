<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#FEF7F8"
        android:layout_margin="@dimen/dp10"
       >
    <LinearLayout
        android:id="@+id/select_ll"
        android:layout_width="168dp"
        android:layout_height="@dimen/dp220"
        android:orientation="vertical"
        android:background="@drawable/shape_1aff3f63_10_my"
        >
        <ImageView
            android:id="@+id/ivlike"
            android:layout_width="168dp"
            android:layout_height="195dp"
            android:src="@mipmap/my_vip_regrets"
            android:layout_marginTop="-10dp"
            android:background="@drawable/shape_1aff3f63_10_myl_ike"
            />
        <TextView
            android:id="@+id/tvLikeName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="于晓，24"
            android:textColor="@color/text_black_333"
            android:textSize="@dimen/sp_14_text_size"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="@dimen/dp5"
            android:layout_marginBottom="@dimen/dp10"
            />
    </LinearLayout>
</LinearLayout>